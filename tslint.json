{
  "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "node_modules",
      "__generated__"
    ]
  },
  "rules": {
    "variable-name": [true, "ban-keywords", "check-format", "allow-pascal-case"],
    "import-name": false,
    "ordered-imports": [
      true,
      {
        "import-sources-order": "lowercase-first",
        "named-imports-order": "lowercase-first"
      }
    ]
  }
}
